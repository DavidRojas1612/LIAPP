    "apollo-boost": "^0.4.4",
    "babel-plugin-module-resolver": "^3.2.0",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.5.2",
    "firebase": "^7.4.0",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "history": "^4.10.1",
    "i18next": "^13.1.5",
    "node-sass": "^4.13.0",
    "postcss": "^7.0.21",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "3.2.0",
    "react-spinkit": "^3.0.0",
    "redux": "^4.0.4",
    "redux-beacon": "^2.1.0",
    "redux-form": "^8.2.6",
    "redux-recompose": "^2.2.0",
    "redux-thunk": "^2.3.0",
    "seamless-immutable": "^7.1.4",
    "wolox-equalizer": "0.0.3"
  },
  "scripts": {
    "start": "node ./scripts/start.js development",
    "start-env": "node ./scripts/start.js",
    "build": "react-scripts build && npm run build-sw",
    "clean-cra-sw": "rm -f build/precache-manifest.*.js && rm -f build/service-worker.js",
    "build-sw": "node ./src/sw-build.js",
    "predeploy" : "npm run build",
    "deploy": "gh-pages -d build",
    "test": "rescripts test --env=jsdom",
    "eject": "./node_modules/react-scripts/bin/react-scripts.js eject",
    "lint": "./node_modules/eslint/bin/eslint.js 'src/**'",
    "lint-fix": "./node_modules/eslint/bin/eslint.js src --fix",
    "lint-scss": "./node_modules/stylelint/bin/stylelint.js '**/*.scss' --fix",
    "lint-diff": "git diff --name-only --cached --relative --diff-filter=ACM | grep \\.js$ | xargs eslint"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.7.4",
    "@rescripts/cli": "0.0.7",
    "aws-deploy-script-fe": "0.0.4",
    "chalk": "^2.4.2",
    "env-cmd": "^9.0.3",
    "eslint-config-wolox": "^3.0.2",
    "eslint-config-wolox-react": "^2.0.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^3.9.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-native": "^3.5.0",
    "gh-pages": "^2.1.1",
    "husky": "^2.7.0",
    "minimist": "^1.2.0",
    "postcss-html": "^0.36.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^1.17.1",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "prettier-stylelint": "^0.4.2",
    "prop-types": "^15.7.2",
    "react-hot-loader": "^4.12.15",
    "stylelint": "^9.10.1",
    "stylelint-config-wolox": "^1.0.5",
    "stylelint-no-indistinguishable-colors": "^1.2.1",
    "stylelint-scss": "^3.5.4"
  },
  "title": "liapp",
  "repository": {
    "type": "git"
  },
  "engines": {
    "node": ">= 10.15.3",
    "npm": ">= 6.9.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-diff && npm run lint-scss"
    }
  }
}
